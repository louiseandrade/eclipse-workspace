
public class TestaVariaveis {
	
	public static void main(String[] args) {
		System.out.print("ola novo teste");
		
		int idade;
		idade = 37;
		
		System.out.print(idade);
		
		idade = 30 + 10;
		
		System.out.print(idade);

		idade = (7*5) + 2;
		
		System.out.print(idade);
		
		System.out.print("a idade é " + idade);
	}
}
